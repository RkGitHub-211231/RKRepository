<aura:component >
    <aura:attribute name="salary" type="integer"/>
    <aura:attribute name="empname" type="string"/>
    <aura:attribute name="bonus" type="decimal"/>
    <aura:attribute name="exp" type="integer"/>
    <aura:handler name="init" value="{!this}" action="{!c.setter}" />
    
    empname: {!v.empname}
    salary: {!v.salary}
    exp: {!v.exp}
    bonus: {!v.bonus}
</aura:component>